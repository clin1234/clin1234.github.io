{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: gcc.exe build active file",
			"command": "C:\\Strawberry\\c\\bin\\gcc.exe",
			"args": [
				"-g",
				"${file}",
				"-Wall",
				"-Wextra",
				"-pedantic",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "C:\\Strawberry\\c\\bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind":"build",
				"isDefault":true
			}
		}
	]
}